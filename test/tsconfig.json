{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "lib": [
      "ES6"
    ],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "outDir": "./dist/",
    "inlineSourceMap": true,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  }
}